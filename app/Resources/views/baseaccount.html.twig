        {%endverbatim%}
        <link href="{{asset('favicon.ico')}}" rel="shortcut icon" type="image/x-icon" >
	<link rel="icon" href="{{asset('favicon.ico')}}" type="image/x-icon">
        <!-- START @GLOBAL MANDATORY STYLES -->
        <link data-ng-href="{{asset('vendor/bootstrap/dist/css/bootstrap.min.css')}}" rel="stylesheet">
        <link rel="stylesheet" href="{{asset('plugin/font-awesome/css/font-awesome.css')}}" />
        <link rel="stylesheet" href="{{asset('plugin/animate.css/animate.css')}}" />
        <link rel="stylesheet" href="{{asset('plugin/angular-loading-bar/build/loading-bar.css')}}" />
        <link rel="stylesheet" href="{{asset('css/flaticon.css')}}" />
        {%verbatim%}
        <!--/ END GLOBAL MANDATORY STYLES -->
        
        <!-- START @PAGE LEVEL STYLES -->
        <link id="load_css_before"/>
        <!--/ END PAGE LEVEL STYLES -->

        <!-- START @THEME STYLES -->
        {%endverbatim%}
        {% if(app.environment=='dev') %}    
        <link data-ng-href="{{asset('app/styles/themes/blue.theme.css')}}" rel="stylesheet" id="theme">
        {%else%}
        <link data-ng-href="{{asset('build/styles/themes/blue.theme.css')}}" rel="stylesheet" id="theme">
        {%endif%}
        
        <!--/ END THEME STYLES -->

        <!-- START @ANGULARJS STYLES -->
        <link data-ng-href="{{asset('css/compiled/angular-custom.css')}}" rel="stylesheet">
