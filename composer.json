{
    "name": "usuario/multiacademico",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.1.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/assetic-bundle": "^2.7",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "nelmio/cors-bundle": "^1.4",
        
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "friendsofsymfony/jsrouting-bundle": "dev-master",
        "friendsofsymfony/rest-bundle": "^1.7",
        "friendsofsymfony/oauth-server-bundle": "dev-master",
        
        "fresh/doctrine-enum-bundle": "~4.1",
        "google/apiclient":"2.0",
        "knplabs/knp-snappy-bundle": "dev-master",        
        
        
        "components/jquery": "1.11.1",
        "datatables/datatables": "1.10.4",
        "moment/moment": "2.8.4",
        "renearias/arxis-datatables-bundle": "dev-master",
        "jms/serializer-bundle": "^1.0",
        
        "ircmaxell/password-compat": "^1.0",
        
        "knplabs/knp-time-bundle": "^1.5",
        "doctrine/doctrine-fixtures-bundle": "^2.2",
        "twig/extensions": "^1.3",
        "h4cc/wkhtmltopdf-amd64": "0.12.3",
        "h4cc/wkhtmltopdf-i386": "0.12.3",
        "h4cc/wkhtmltoimage-amd64": "0.12.3",
        "h4cc/wkhtmltoimage-i386": "0.12.3",
        "wemersonjanuario/wkhtmltopdf-windows": "dev-master",
        "snc/redis-bundle": "2.x-dev",
        "hwi/oauth-bundle": "dev-master",
        "kreait/firebase-php": "1.0",
        "firebase/token-generator": "^3.0"

    },
    "require-dev": {
         "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^2.7"
    },
     "scripts": {
        "compile": [
            "bin/console assetic:dump"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "env-map": {
                "database_host": "DATABASE_HOST",
                "database_name": "DATABASE_NAME",
                "database_user": "DATABASE_USER",
                "database_password": "DATABASE_PASS",
                "firebase_host": "multiacademico-test.firebaseio.com",
                "firebase_service_json": "%kernel.root_dir%/Resources/FireBase/MultiacademicoTest-3fe0ede53e47.json",
                "oauth_client": "OAUTH_CLIENT",
                "oauth_secret": "OAUTH_SECRET",
                "website_back_base_url": "WEBSITE_BACK_URL"
            }
        }
    }
}
