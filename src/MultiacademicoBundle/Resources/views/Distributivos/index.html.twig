{% block content -%}
<div id="content">
    <div class="row">
        <big-breadcrumbs items="['Distributivos', 'Resumen']" icon="file"
                         class="col-xs-12 col-sm-7 col-md-7 col-lg-4"></big-breadcrumbs>
            </div>
    <div class="row">

    <table class="table records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Distributivoparalelo</th>
                <th>Distributivoseccion</th>
                <th>Distributivohora</th>
                <th>Distributivofecha</th>
                <th>Distributivoestado</th>
                <th>Distributivogrado</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('distributivos_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.distributivoparalelo }}</td>
                <td>{{ entity.distributivoseccion }}</td>
                <td>{{ entity.distributivohora }}</td>
                <td>{{ entity.distributivofecha }}</td>
                <td>{{ entity.distributivoestado }}</td>
                <td>{{ entity.distributivogrado }}</td>
                <td>
                <ul>                    
                    
                    
                    
                    
                                                                                                            
                                             
                    <li>
                        <a href="{{ path('distributivos_show', { 'id': entity.id }) }}">mostrar</a>
                    </li>                    
                    
                    
                    
                    
                                                                                                            
                                             
                    <li>
                        <a href="{{ path('distributivos_edit', { 'id': entity.id }) }}">editar</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    </div>
    <div class="row">    
        <ul>
        <li>
            <a href="{{ path('distributivos_new') }}">
                Crear nueva entrada
            </a>
        </li>
    </ul>
        </div>
</div>    
{% endblock %}
