              </tr>
         </thead> 
         <tbody>
      <tr data-ng-repeat="calificacion in estudiante.calificaciones|ordenarCalificaciones">
          {%verbatim%}
          <td >{{$index+1}}</td>
          <td>{{calificacion.calificacioncodmateria.materia}}</td>
          <td data-ng-if="p<=3" style="color:#000000">{{cualitativa(prparcial(q,p,calificacion))}}</td>
          <td data-ng-if="p<=3" style="color:#000000">{{prparcial(q,p,calificacion)}}</td>
          <td data-ng-if="p<=3" >{{calificacion['q'+q+'_p'+p+'_planmejora']}}</td>
          <td data-ng-if="p<=3" >{{calificacion['q'+q+'_p'+p+'_recomendacion']}}</td>
          
          
          <td data-ng-if="p==4" style="color:#000000">{{prparcial(q,1,calificacion)}}</td>
          <td data-ng-if="p==4" style="color:#000000">{{prparcial(q,2,calificacion)}}</td>
          <td data-ng-if="p==4" style="color:#000000">{{prparcial(q,3,calificacion)}}</td>
          <td data-ng-if="p==4" style="color:#000000">{{prparciales(q,calificacion)}}</td>
          <td data-ng-if="p==4" style="color:#000000">{{prparciales80(q,calificacion)}}</td>
          <td data-ng-if="p==4" style="color:#000000">{{calificacion['q'+q+'_ex']}}</td>
          <td data-ng-if="p==4" style="color:#000000">{{ex20(q,calificacion)}}</td>
          <td data-ng-if="p==4" style="color:#000000">{{prq(q,calificacion)}}</td>
          <td data-ng-if="p==4" style="color:#000000">{{cualitativasiglas(prq(q,calificacion))}}</td>
          <td data-ng-if="p==4">{{calificacion['q'+q+'_planmejora']}}</td>
          <td data-ng-if="p==4" >{{calificacion['q'+q+'_recomendacion']}}</td>
          {%endverbatim%}
          
      </tr>
